<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<fx:root stylesheets="@../style.css"
        prefHeight="850.0" prefWidth="1500.0"
         type="javafx.scene.layout.AnchorPane"
         xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1">
    <ScrollPane fx:id="scrollPane" fitToHeight="true" maxWidth="375"
                style="-fx-border-width: 5; -fx-border-color: #c0c0c0; -fx-padding: 5"
                AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <HBox fx:id="collectiblesContainer" spacing="10" />
    </ScrollPane>
</fx:root>
