<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<fx:root stylesheets="@../style.css"

         prefHeight="1000.0" prefWidth="1400.0"
         style="-fx-border-width: 1px; -fx-border-color: black;"
         type="javafx.scene.layout.AnchorPane"
         xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
    <ScrollPane fx:id="scrollPane" fitToHeight="true" fitToWidth="true" style="-fx-border-width: 1px; -fx-border-color: red;"
                AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <GridPane fx:id="photosContainer" hgap="20" prefHeight="200" prefWidth="200" vgap="20">
         <padding>
            <Insets left="20.0" top="20.0" bottom="20.0" />
         </padding>
        </GridPane>
    </ScrollPane>
</fx:root>
