<?xml version="1.0" encoding="UTF-8"?>

<?import com.example.fotoradar.components.TeamFormComponent?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.fotoradar.windows.TeamFormWindow"
            prefHeight="400.0" prefWidth="600.0">
    <Label layoutY="14.0" text="Dodaj zespół"
           alignment="CENTER" AnchorPane.rightAnchor="0" AnchorPane.leftAnchor="0"/>
    <TeamFormComponent fx:id="teamForm" layoutY="30.0" />
    <Button layoutX="127.0" layoutY="468.0" mnemonicParsing="false" text="ZAPISZ" onAction="#saveTeam" />
    <Button layoutX="208.0" layoutY="468.0" mnemonicParsing="false" text="ANULUJ" onAction="#cancel" />
</AnchorPane>
