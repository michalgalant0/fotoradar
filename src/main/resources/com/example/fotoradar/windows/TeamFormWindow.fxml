<?xml version="1.0" encoding="UTF-8"?>

<?import com.example.fotoradar.components.TeamFormComponent?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import java.net.URL?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" >
    <stylesheets>
        <URL value="@../styles/globalStyles.css" />
        <URL value="@../styles/windows/TeamFormWindowStyles.css" />
    </stylesheets>

        <TeamFormComponent fx:id="teamForm" />
        <HBox spacing="16.0" alignment="CENTER"
              AnchorPane.bottomAnchor="16.0"
              AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0">
            <Button mnemonicParsing="false" text="ZAPISZ" onAction="#saveTeam" />
            <Button mnemonicParsing="false" text="ANULUJ" onAction="#cancel" />
        </HBox>
</AnchorPane>
