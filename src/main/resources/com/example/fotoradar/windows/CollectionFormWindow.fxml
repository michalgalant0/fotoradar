<?xml version="1.0" encoding="UTF-8"?>

<?import com.example.fotoradar.components.CollectionFormComponent?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import java.net.URL?>
<?import javafx.scene.layout.HBox?>
<AnchorPane prefWidth="400.0"
            xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@../styles/globalStyles.css" />
        <URL value="@../styles/windows/CollectionFormWindowStyles.css" />
    </stylesheets>

    <CollectionFormComponent fx:id="collectionForm" />
    <HBox spacing="16.0" AnchorPane.bottomAnchor="16.0" prefWidth="400"
        alignment="BASELINE_CENTER">
        <Button mnemonicParsing="false" text="ZAPISZ" onAction="#saveCollection" />
        <Button mnemonicParsing="false" text="ANULUJ" onAction="#cancel" />
    </HBox>
</AnchorPane>
